{
  "v": "4.0",
  "name": "summit2019lab",
  "workspace": {
    "defaultEnv": "default",
    "environments": {
      "default": {
        "machines": {
          "dev-machine": {
            "attributes": {
              "memoryLimitBytes": "3221225472"
            },
            "servers": {
              "springboot": {
                "port": "8080",
                "attributes": {},
                "protocol": "http"
              }
            },
            "volumes": {
              "m2": {
                "path": "/home/jboss/.m2"
              },
              "javadata": {
                "path": "/home/jboss/jdtls/data"
              }
            },
            "installers": [
              "org.eclipse.che.exec",
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ls.camel",
              "org.eclipse.che.ls.java",
              "com.redhat.bayesian.lsp"
            ],
            "env": {}
          }
        },
        "recipe": {
          "type": "dockerimage",
          "content": "registry.access.redhat.com/codeready-workspaces/stacks-java"
        }
      }
    },
    "projects": [
      {
        "links": [],
        "name": "summit2019-lab",
        "attributes": {
          "contribute_to_branch": [
            "master"
          ],
          "commands": [
            "{\"commandLine\":\"wget https://github.com/apache/camel-k/releases/download/0.3.2/camel-k-client-0.3.2-linux-64bit.tar.gz \\nchmod 755 camel-k-client-0.3.2-linux-64bit.tar.gz \\nmkdir bin \\ntar -xf camel-k-client-0.3.2-linux-64bit.tar.gz -C bin/ \\necho \\\"PATH=$PATH:/projects/bin\\\" >> ~/.bash_profile \\necho \\\"export PATH\\\" >> ~/.bash_profile\\nsource ~/.bash_profile\\necho \\\"---DONE---\\\"\", \"name\":\"InstallCamelK\", \"attributes\":{\"goal\":\"Common\", \"previewUrl\":\"\"}, \"type\":\"custom\"}"
          ]
        },
        "type": "blank",
        "source": {
          "location": "https://github.com/weimeilin79/summit2019-lab.git",
          "type": "git",
          "parameters": {}
        },
        "path": "/summit2019-lab",
        "problems": [],
        "mixins": [
          "pullrequest"
        ]
      }
    ],
    "name": "Summitlab",
    "attributes": {},
    "commands": [
      {
        "commandLine": "MAVEN_OPTS='-Xmx100m' && mvn clean package -f ${current.project.path}/pom.xml",
        "name": "build",
        "attributes": {
          "goal": "Build",
          "previewUrl": ""
        },
        "type": "mvn"
      },
      {
        "commandLine": "wget https://github.com/apache/camel-k/releases/download/0.3.2/camel-k-client-0.3.2-linux-64bit.tar.gz \nchmod 755 camel-k-client-0.3.2-linux-64bit.tar.gz \nmkdir bin \ntar -xf camel-k-client-0.3.2-linux-64bit.tar.gz -C bin/ \necho \"PATH=$PATH:/projects/bin\" >> ~/.bash_profile \necho \"export PATH\" >> ~/.bash_profile\nsource ~/.bash_profile",
        "name": "InstallCamelK",
        "attributes": {
          "previewUrl": ""
        },
        "type": "custom"
      }
    ],
    "links": []
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [
        {
          "properties": {
            "name": "InstallCamelK"
          },
          "id": "runCommand"
        }
      ]
    }
  }
}